{
    "name": "Notoy-Chrome",
    "description": "Notoy Chrome browser extension.",
    "version": "0.9.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs",
        "downloads"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}
